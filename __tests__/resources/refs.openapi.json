{
  "openapi": "3.0.3",
  "info": {
    "title": "Circular References",
    "description": "API with circular references",
    "version": "1.0.0"
  },
  "paths": {
    "/items": {
      "get": {
        "operationId": "getItems",
        "summary": "List items",
        "description": "Returns all items in database",
        "responses": {
          "200": {
            "description": "List of items"
          }
        },
        "parameters": [
          {
            "name": "filter",
            "in": "query",
            "description": "Filter items by FilterExpression",
            "required": false,
            "schema": {
              "$ref": "#/components/schemas/FilterExpression"
            }
					},
					{
						"name": "remoteReference",
						"in": "query",
						"description": "",
						"required": false,
						"schema": {
							"$ref": "__tests__/resources/example-pet-api.openapi.json#/components/schemas/PetId"
						}
					}
        ]
      }
    }
  },
  "components": {
    "schemas": {
      "FilterExpression": {
        "anyOf": [
          {
            "$ref": "#/components/schemas/FilterExpressionValue"
          },
          {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/FilterExpressionValue"
            }
          }
        ]
      },
      "FilterExpressionValue": {
        "anyOf": [
          {
            "$ref": "#/components/schemas/FilterExpression"
          },
          {
            "type": "string"
          },
          {
            "type": "number"
          }
        ]
      }
    }
  }
}